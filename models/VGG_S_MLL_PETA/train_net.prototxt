name: "VGG_S_MLL_PETA"

layer {
  name: "input-data"
  type: "Python"
  top: "data"
  top: "attr"
  top: "weight"
  python_param {
    module: "data_layer.layer"
    layer: "DataLayer"
  }
}
########################## VGG CNN S ##########################
layer {
  bottom: "data"
  top: "conv1"
  name: "conv1"
  type: "Convolution"
  param {
    lr_mult: 0.0
    decay_mult: 0.0
  }
  param {
    lr_mult: 0.0
    decay_mult: 0
  }
  convolution_param {
    num_output: 96
    kernel_size: 7
    stride: 2
  }
}
layer {
  bottom: "conv1"
  top: "conv1"
  name: "ReLU1"
  type: "ReLU"
}
layer {
  bottom: "conv1"
  top: "norm1"
  name: "norm1"
  type: "LRN"
  lrn_param {
    local_size: 5
    alpha: 0.0005
    beta: 0.75
    k: 2
  }
}
layer {
  bottom: "norm1"
  top: "pool1"
  name: "pool1"
  type: "Pooling"
  pooling_param {
    pool: MAX
    kernel_size: 3
    stride: 3
  }
}
layer {
  bottom: "pool1"
  top: "conv2"
  name: "conv2"
  type: "Convolution"
  param {
    lr_mult: 0.1
    decay_mult: 0.1
  }
  param {
    lr_mult: 0.2
    decay_mult: 0
  }
  convolution_param {
    num_output: 256
    pad: 2
    kernel_size: 5
  }
}
layer {
  bottom: "conv2"
  top: "conv2"
  name: "ReLU2"
  type: "ReLU"
}
layer {
  bottom: "conv2"
  top: "pool2"
  name: "pool2"
  type: "Pooling"
  pooling_param {
    pool: MAX
    kernel_size: 2
    stride: 2
  }
}
layer {
  bottom: "pool2"
  top: "conv3"
  name: "conv3"
  type: "Convolution"
  param {
    lr_mult: 0.2
    decay_mult: 0.2
  }
  param {
    lr_mult: 0.4
    decay_mult: 0
  }
  convolution_param {
    num_output: 512
    pad: 1
    kernel_size: 3
  }
}
layer {
  bottom: "conv3"
  top: "conv3"
  name: "ReLU3"
  type: "ReLU"
}
layer {
  bottom: "conv3"
  top: "conv4"
  name: "conv4"
  type: "Convolution"
  param {
    lr_mult: 0.3
    decay_mult: 0.3
  }
  param {
    lr_mult: 0.6
    decay_mult: 0
  }
  convolution_param {
    num_output: 512
    pad: 1
    kernel_size: 3
  }
}
layer {
  bottom: "conv4"
  top: "conv4"
  name: "ReLU4"
  type: "ReLU"
}
layer {
  bottom: "conv4"
  top: "conv5"
  name: "conv5"
  type: "Convolution"
  param {
    lr_mult: 0.4
    decay_mult: 0.4
  }
  param {
    lr_mult: 0.8
    decay_mult: 0
  }
  convolution_param {
    num_output: 512
    pad: 1
    kernel_size: 3
  }
}
layer {
  bottom: "conv5"
  top: "conv5"
  name: "ReLU5"
  type: "ReLU"
}
layer {
  bottom: "conv3"
  top: "pool3"
  name: "pool3"
  type: "Pooling"
  pooling_param {
    pool: AVE
    kernel_size: 2
    stride: 2
  }
}
layer {
  bottom: "conv4"
  top: "pool4"
  name: "pool4"
  type: "Pooling"
  pooling_param {
    pool: AVE
    kernel_size: 2
    stride: 2
  }
}
layer {
  bottom: "conv5"
  top: "pool5"
  name: "pool5"
  type: "Pooling"
  pooling_param {
    pool: AVE
    kernel_size: 2
    stride: 2
  }
}
########################## Feature Transform Conv Layers ##########################
layer {
  bottom: "pool3"
  top: "conv3_e"
  name: "conv3_e"
  type: "Convolution"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    weight_filler {
        type: "msra"
    }
    bias_filler {
        type: "constant"
        value: 0
    }
    num_output: 512
    pad: 1
    kernel_size: 3
  }
}
layer {
  bottom: "conv3_e"
  top: "conv3_e"
  name: "ReLU3_e"
  type: "ReLU"
}
layer {
  bottom: "pool4"
  top: "conv4_e"
  name: "conv4_e"
  type: "Convolution"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    weight_filler {
        type: "msra"
    }
    bias_filler {
        type: "constant"
        value: 0
    }
    num_output: 512
    pad: 1
    kernel_size: 3
  }
}
layer {
  bottom: "conv4_e"
  top: "conv4_e"
  name: "ReLU4_e"
  type: "ReLU"
}
layer {
  bottom: "pool5"
  top: "conv5_e"
  name: "conv5_e"
  type: "Convolution"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    weight_filler {
        type: "msra"
    }
    bias_filler {
        type: "constant"
        value: 0
    }
    num_output: 1024
    pad: 1
    kernel_size: 3
  }
}
layer {
  bottom: "conv5_e"
  top: "conv5_e"
  name: "ReLU5_e"
  type: "ReLU"
}
########################## Global Pooling Layers ##########################
layer {
  bottom: "conv3_e"
  top: "pool3_e"
  name: "pool3_e"
  type: "Pooling"
  pooling_param {
    pool: MAX
    global_pooling: 1
  }
}
layer {
  bottom: "pool3_e"
  top: "pool3_e"
  name: "drop3_p"
  type: "Dropout"
  dropout_param {
    dropout_ratio: 0.5
  }
}
layer {
  bottom: "conv4_e"
  top: "pool4_e"
  name: "pool4_e"
  type: "Pooling"
  pooling_param {
    pool: MAX
    global_pooling: 1
  }
}
layer {
  bottom: "pool4_e"
  top: "pool4_e"
  name: "drop4_p"
  type: "Dropout"
  dropout_param {
    dropout_ratio: 0.5
  }
}
layer {
  bottom: "conv5_e"
  top: "pool5_e"
  name: "pool5_e"
  type: "Pooling"
  pooling_param {
    pool: MAX
    global_pooling: 1
  }
}
layer {
  bottom: "pool5_e"
  top: "pool5_e"
  name: "drop5_p"
  type: "Dropout"
  dropout_param {
    dropout_ratio: 0.5
  }
}
########################## Independent FC Layers ##########################
layer {
  bottom: "pool3_e"
  top: "fc3_e"
  name: "fc3_e"
  type: "InnerProduct"
  param {
    lr_mult: 0.5
    decay_mult: 0.5
  }
  param {
    lr_mult: 1
    decay_mult: 0
  }
  inner_product_param {
    weight_filler {
        type: "gaussian"
        std: 0.005
    }
    bias_filler {
        type: "constant"
        value: 0.1
    }
    num_output: 512
  }
}
layer {
  bottom: "fc3_e"
  top: "fc3_e"
  name: "relu3_e"
  type: "ReLU"
}
layer {
  bottom: "fc3_e"
  top: "fc3_e"
  name: "drop3"
  type: "Dropout"
  dropout_param {
    dropout_ratio: 0.5
  }
}
layer {
  bottom: "pool4_e"
  top: "fc4_e"
  name: "fc4_e"
  type: "InnerProduct"
  param {
    lr_mult: 0.5
    decay_mult: 0.5
  }
  param {
    lr_mult: 1
    decay_mult: 0
  }
  inner_product_param {
    weight_filler {
        type: "gaussian"
        std: 0.005
    }
    bias_filler {
        type: "constant"
        value: 0.1
    }
    num_output: 512
  }
}
layer {
  bottom: "fc4_e"
  top: "fc4_e"
  name: "relu4_e"
  type: "ReLU"
}
layer {
  bottom: "fc4_e"
  top: "fc4_e"
  name: "drop4"
  type: "Dropout"
  dropout_param {
    dropout_ratio: 0.5
  }
}
layer {
  bottom: "pool5_e"
  top: "fc5_e"
  name: "fc5_e"
  type: "InnerProduct"
  param {
    lr_mult: 0.5
    decay_mult: 0.5
  }
  param {
    lr_mult: 1
    decay_mult: 0
  }
  inner_product_param {
    weight_filler {
        type: "gaussian"
        std: 0.005
    }
    bias_filler {
        type: "constant"
        value: 0.1
    }
    num_output: 1024
  }
}
layer {
  bottom: "fc5_e"
  top: "fc5_e"
  name: "relu5_e"
  type: "ReLU"
}
layer {
  bottom: "fc5_e"
  top: "fc5_e"
layer {
  bottom: "pool3_e"
  top: "pool3_e"
  name: "relu3_e"
  type: "ReLU"
}
  name: "drop5"
  type: "Dropout"
  dropout_param {
    dropout_ratio: 0.5
  }
}
########################## Synthesis Layers ##########################
layer {
  name: "concat"
  bottom: "fc3_e"
  bottom: "fc4_e"
  bottom: "fc5_e"
  top: "concat"
  type: "Concat"
}
layer {
  bottom: "concat"
  top: "fc_syn1"
  name: "fc_syn1"
  type: "InnerProduct"
  param {
    lr_mult: 0.5
    decay_mult: 0.5
  }
  param {
    lr_mult: 1
    decay_mult: 0
  }
  inner_product_param {
    weight_filler {
        type: "gaussian"
        std: 0.005
    }
    bias_filler {
        type: "constant"
        value: 0.1
    }
    num_output: 2048
  }
}
layer {
  bottom: "fc_syn1"
  top: "fc_syn1"
  name: "drop"
  type: "Dropout"
  dropout_param {
    dropout_ratio: 0.5
  }
}
layer {
  bottom: "fc_syn1"
  top: "fc_syn2"
  name: "fc_syn2"
  type: "InnerProduct"
  param {
    lr_mult: 0.5
    decay_mult: 0.5
  }
  param {
    lr_mult: 1
    decay_mult: 0
  }
  inner_product_param {
    weight_filler {
        type: "gaussian"
        std: 0.005
    }
    bias_filler {
        type: "constant"
        value: 0.1
    }
    num_output: 35
  }
}
########################## Loss Layer ##########################
layer {
  bottom: "fc_syn2"
  bottom: "attr"
  bottom: "weight"
  name: "loss"
  top: "loss"
  type: "SigmoidWeightedCrossEntropyLoss"
}
