net: "models/GOOGLENET_SPP_PETA/train_net.prototxt"

#regularization_type: "L1"
base_lr: 0.0005
lr_policy: "step"
gamma: 0.75
stepsize: 20000
display: 100
average_loss: 100
momentum: 0.9
weight_decay: 0.0005

# Standard caffe solver snapshotting is replaced by our own snapshot function
snapshot: 0

# We still use the snapshot prefix, though
snapshot_prefix: "googlenet_spp_peta"
